{
  "name": "@specter/nextjs-with-redux",
  "version": "0.6.5",
  "private": true,
  "scripts": {
    "bundlesize": "ANALYZE=true npm run build",
    "build": "rm -rf ./dist/* && run-p build:next:*",
    "build:next:server": "NODE_ENV=production tsc --project tsconfig.server.json",
    "build:next:client": "NODE_ENV=production next build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "lint": "run-p lint:*",
    "lint:sass": "stylelint \"src/**/*.tsx\" \"src/**/*.ts\"",
    "lint:typecheck": "tsc -p . --noEmit",
    "production": "NODE_ENV=production node ./dist/src/express/server.js",
    "start": "nodemon",
    "test": "jest --maxWorkers=4 --coverage --verbose=false --silent=false",
    "watch": "run-p -r 'watch:*'",
    "watch:boot": "cd ../spec && npm run start:server",
    "watch:jest": "jest --maxWorkers=4 --watch --verbose=false --silent=false"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
