    "@types/js-cookie": "^2.2.6",
    "axios": "^0.21.1",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "csurf": "^1.11.0",
    "date-fns": "^2.13.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "favalid": "^0.9.1",
    "ioredis": "^4.16.3",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "memorystore": "^1.6.2",
    "next": "^9.4.0",
    "next-pwa": "^2.4.0",
    "path-to-regexp": "^6.1.0",
    "pino": "^5.17.0",
